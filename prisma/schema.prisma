datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") 
  directUrl = env("POSTGRES_URL_NON_POOLING") 
}

generator client {
  provider = "prisma-client-js"
}

model User {
    id             String   @id @default(uuid()) 
    institution    String   
    name_conductor String   
    name_dog       String  
    age_dog        Int      
    sex_dog       String  
}
